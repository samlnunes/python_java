package estrutura_repeticao;

import java.util.Scanner;

public class Ex48 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		int num, i,;
		
		System.out.printf("Digite um n√∫mero positivo: ");
	    num = ler.nextInt();
	    
	    for (i=1; i<=num; i++) {
	    	
            System.out.print(i);
        }

	}

}
